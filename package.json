{
  "name": "assert-log",
  "version": "0.2.0",
  "description": "A simple way to unit-test callbacks",
  "keywords": [
    "assert",
    "callback",
    "unit-test"
  ],
  "homepage": "https://github.com/swansontec/assert-log#readme",
  "bugs": {
    "url": "https://github.com/swansontec/assert-log/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:swansontec/assert-log.git"
  },
  "license": "MIT",
  "author": "William Swanson",
  "contributors": [
    "William Swanson <william@edge.app>"
  ],
  "files": [
    "CHANGELOG.md",
    "lib/*",
    "package.json",
    "README.md",
    "src/flow-types.js"
  ],
  "main": "lib/assert-log.cjs.js",
  "module": "lib/assert-log.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts .",
    "precommit": "lint-staged && npm test && npm run prepare",
    "prepare": "rimraf lib && rollup -c && tsc",
    "test": "mocha -r sucrase/register test/**/*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": ">=2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": ">=10.0.0",
    "babel-plugin-transform-fake-error-class": "^0.1.0",
    "chai": "^4.2.0",
    "eslint": ">=6.2.2",
    "eslint-config-standard-kit": ">=0.14.2",
    "eslint-plugin-flowtype": ">=4.3.0",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-prettier": ">=3.0.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-simple-import-sort": ">=4.0.0",
    "eslint-plugin-standard": ">=4.0.0",
    "husky": ">=3.0.0",
    "lint-staged": ">=9.0.0",
    "mocha": "^6.2.2",
    "prettier": ">=1.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.8",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-flow-entry": "^0.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sucrase": "^3.10.1",
    "typescript": "^3.7.2"
  }
}
