{
  "name": "assert-log",
  "version": "0.2.1",
  "description": "A simple way to unit-test callbacks",
  "keywords": [
    "assert",
    "callback",
    "unit-test"
  ],
  "homepage": "https://github.com/swansontec/assert-log#readme",
  "bugs": {
    "url": "https://github.com/swansontec/assert-log/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:swansontec/assert-log.git"
  },
  "license": "MIT",
  "author": "William Swanson",
  "contributors": [
    "William Swanson <william@edge.app>"
  ],
  "main": "lib/assert-log.cjs.js",
  "module": "lib/assert-log.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "lib/*",
    "package.json",
    "README.md",
    "src/flow-types.js"
  ],
  "scripts": {
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts .",
    "precommit": "lint-staged && npm test && npm run prepare",
    "prepare": "rimraf lib && rollup -c && tsc",
    "test": "mocha -r sucrase/register test/**/*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": ">=2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": ">=10.1.0",
    "babel-plugin-transform-fake-error-class": "^0.1.0",
    "chai": "^4.2.0",
    "eslint": ">=6.8.0",
    "eslint-config-standard-kit": ">=0.14.2",
    "eslint-plugin-flowtype": ">=4.6.0",
    "eslint-plugin-import": ">=2.20.1",
    "eslint-plugin-prettier": ">=3.1.2",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-simple-import-sort": ">=5.0.2",
    "eslint-plugin-standard": ">=4.0.0",
    "husky": ">=4.2.3",
    "lint-staged": ">=10.0.8",
    "mocha": "^7.1.0",
    "prettier": ">=1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-flow-entry": "^0.3.4",
    "sucrase": "^3.12.1",
    "typescript": "^3.8.3"
  }
}
